{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "bash scripts/fix-lint-issues.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm -- -d ./typeorm.config.ts migration:create",
    "migration:generate": "npm run typeorm -- -d ./typeorm.config.ts migration:generate",
    "migration:run": "npm run typeorm -- -d ./typeorm.config.ts migration:run",
    "migration:revert": "npm run typeorm -- -d ./typeorm.config.ts migration:revert",
    "db:sync": "cross-env DB_SYNC=true npm run start:dev",
    "db:sync:prod": "cross-env DB_SYNC=true npm run start:prod",
    "db:reset": "cross-env DB_SYNC=true DB_DROP_SCHEMA=true npm run start:dev",
    "prepare": "husky"
  },
  "dependencies": {
