    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^11.10.0",
    "bullmq": "^5.56.4",
    "cache-manager": "^7.0.1",
    "cache-manager-redis-store": "^3.0.1",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "ejs": "^3.1.10",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
