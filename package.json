    "@aws-sdk/client-secrets-manager": "^3.828.0",
    "@aws-sdk/client-ses": "^3.828.0",
    "@aws-sdk/client-sns": "^3.828.0",
    "@aws-sdk/client-sqs": "^3.828.0",
    "@aws-sdk/credential-providers": "^3.828.0",
    "@aws-sdk/lib-storage": "^3.828.0",
    "@aws-sdk/s3-request-presigner": "^3.828.0",
    "@bull-board/api": "^6.11.1",
    "@bull-board/express": "^6.11.1",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
