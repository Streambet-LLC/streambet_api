{
  "containerDefinitions": [
    {
      "name": "streambet-prod",
      "image": "<IMAGE_URI>",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        { "name": "NODE_ENV", "value": "development" },
        { "name": "PORT", "value": "3000" },
        { "name": "ENABLE_API_LOGGING", "value": "true" },
        { "name": "ENABLE_DETAILED_LOGGING", "value": "true" },
        { "name": "DB_HOST", "value": "db-private.streambet.tv" },
        { "name": "DB_PORT", "value": "5432" },
        { "name": "JWT_EXPIRES_IN", "value": "1d" },
        { "name": "REFRESH_TOKEN_EXPIRES_IN", "value": "30d" },
        { "name": "FILE_DRIVER", "value": "s3" },
        { "name": "AWS_SMTP_PORT", "value": "465" },
        { "name": "MAIL_DEFAULT_NAME", "value": "Streambet" },
        { "name": "MAIL_SECURE", "value": "true" },
        { "name": "MAIL_REQUIRE_TLS", "value": "true" },
        { "name": "HOSTED", "value": "false" },
        { "name": "REDIS_HOST", "value": "54.172.250.213" },
        { "name": "REDIS_PORT", "value": "6301" },
        { "name": "REDIS_USERNAME", "value": "default" },
        { "name": "REDIS_KEY_PREFIX", "value": "STREAMBET_PROD" },
        { "name": "REDIS_DB", "value": "0" },
        { "name": "IS_SWAGGER_ENABLED", "value": "true" },
        { "name": "IS_BULLMQ_UI_ENABLED", "value": "false" },
        {
          "name": "BLOCKED_REGION",
          "value": "Connecticut,Delaware,Louisiana,Michigan,Montana,Washington,West Virginia"
        },
        { "name": "BLOCK_VPN", "value": "true" },
        { "name": "TRUST_PROXY", "value": "true" },
        { "name": "REDIS_TLS", "value": "false" },
        { "name": "COINFLOW_WEBHOOK_ENV", "value": "prod" }
      ],
      "secrets": [
        { "name": "CLIENT_URL", "valueFrom": "/streambet/prod/CLIENT_URL" },
        { "name": "DB_USERNAME", "valueFrom": "/streambet/prod/DB_USERNAME" },
        { "name": "DB_PASSWORD", "valueFrom": "/streambet/prod/DB_PASSWORD" },
        { "name": "DB_DATABASE", "valueFrom": "/streambet/prod/DB_DATABASE" },
        { "name": "JWT_SECRET", "valueFrom": "/streambet/prod/JWT_SECRET" },
        {
          "name": "REFRESH_TOKEN_SECRET",
          "valueFrom": "/streambet/prod/REFRESH_TOKEN_SECRET"
        },
        {
          "name": "GOOGLE_CLIENT_ID",
          "valueFrom": "/streambet/prod/GOOGLE_CLIENT_ID"
        },
        {
          "name": "GOOGLE_CLIENT_SECRET",
          "valueFrom": "/streambet/prod/GOOGLE_CLIENT_SECRET"
        },
        {
          "name": "GOOGLE_CALLBACK_URL",
          "valueFrom": "/streambet/prod/GOOGLE_CALLBACK_URL"
        },

        {
          "name": "STRIPE_SECRET_KEY",
          "valueFrom": "/streambet/prod/STRIPE_SECRET_KEY"
        },
        {
          "name": "STRIPE_WEBHOOK_SECRET",
          "valueFrom": "/streambet/prod/STRIPE_WEBHOOK_SECRET"
        },
        {
          "name": "ACCESS_KEY_ID",
          "valueFrom": "/streambet/prod/ACCESS_KEY_ID"
        },
        {
          "name": "SECRET_ACCESS_KEY",
          "valueFrom": "/streambet/prod/SECRET_ACCESS_KEY"
        },
        {
          "name": "AWS_DEFAULT_S3_BUCKET",
          "valueFrom": "/streambet/prod/AWS_DEFAULT_S3_BUCKET"
        },
        {
          "name": "AWS_S3_REGION",
          "valueFrom": "/streambet/prod/AWS_S3_REGION"
        },

        {
          "name": "AWS_SMTP_USER",
          "valueFrom": "/streambet/prod/AWS_SMTP_USER"
        },
        {
          "name": "AWS_SMTP_PASSWORD",
          "valueFrom": "/streambet/prod/AWS_SMTP_PASSWORD"
        },
        {
          "name": "AWS_SMTP_HOST",
          "valueFrom": "/streambet/prod/AWS_SMTP_HOST"
        },
        {
          "name": "MAIL_DEFAULT_EMAIL",
          "valueFrom": "/streambet/prod/MAIL_DEFAULT_EMAIL"
        },
        {
          "name": "AWS_SMTP_REGION",
          "valueFrom": "/streambet/prod/AWS_SMTP_REGION"
        },
        {
          "name": "AWS_EMAIL_FROM",
          "valueFrom": "/streambet/prod/AWS_EMAIL_FROM"
        },
        { "name": "APP_HOST_URL", "valueFrom": "/streambet/prod/APP_HOST_URL" },
        {
          "name": "REDIS_PASSWORD",
          "valueFrom": "/streambet/prod/REDIS_PASSWORD"
        },
        {
          "name": "ABSTRACT_API_KEY",
          "valueFrom": "/streambet/prod/ABSTRACT_API_KEY"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/streambet-prod",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "family": "streambet-prod",
  "networkMode": "awsvpc",
  "volumes": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::530976901597:role/ecsTaskExecutionRole"
}
